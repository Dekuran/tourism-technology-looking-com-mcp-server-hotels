# Laravel/PHP project ignores

# Dependencies
/vendor/
/node_modules/

# Environment files
.env
.env.*
!.env.example
gcp-sa-key.json

# Laravel storage (keep placeholder .gitignore files)
/storage/*
!/storage/.gitignore
/storage/app/*
!/storage/app/.gitignore
/storage/app/private/*
!/storage/app/private/.gitignore
/storage/app/public/*
!/storage/app/public/.gitignore
/storage/framework/*
!/storage/framework/.gitignore
/storage/framework/cache/*
!/storage/framework/cache/.gitignore
/storage/framework/sessions/*
!/storage/framework/sessions/.gitignore
/storage/framework/testing/*
!/storage/framework/testing/.gitignore
/storage/framework/views/*
!/storage/framework/views/.gitignore
/storage/logs/*
!/storage/logs/.gitignore

# Laravel cache
/bootstrap/cache/*
!/bootstrap/cache/.gitignore

# Frontend build artifacts
/public/hot
/public/storage
/public/build

# Test and coverage artifacts
.phpunit.result.cache
.phpunit.cache
/coverage/

# Reports (generated by verification scripts)
# Ignore all generated reports, keep folder scaffold
/reports/*
!/reports/.gitkeep

# Editor/project files
.DS_Store
.idea/
.vscode/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Swap/OS files
*.swp
*.swo